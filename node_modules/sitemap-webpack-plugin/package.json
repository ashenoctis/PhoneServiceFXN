{
  "name": "sitemap-webpack-plugin",
  "version": "0.5.1",
  "description": "Webpack plugin to generate a sitemap.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint src/*.js 'test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schneidmaster/sitemap-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "sitemap",
    "static"
  ],
  "author": "Zach Schneider <zach@schneid.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schneidmaster/sitemap-webpack-plugin/issues"
  },
  "files": [
    "lib/",
    "src/",
    "LICENSE.txt",
    "package.json",
    "README.md"
  ],
  "homepage": "https://github.com/schneidmaster/sitemap-webpack-plugin#readme",
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "devDependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.11.15",
    "eslint": "^4.2.0",
    "glob": "^7.1.1",
    "jest": "^20.0.4",
    "pretty-data": "^0.40.0",
    "rimraf": "^2.5.4",
    "timekeeper": "^1.0.0",
    "webpack": "^1.12.14",
    "webpack-stats-plugin": "^0.1.4"
  }
}
